<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Application__c.Volume_Must_Be_Positive</fullName>
    <active>true</active>
    <description>Ensure monthly volume is greater than zero</description>
    <errorConditionFormula>Desired_Monthly_Volume__c &lt;= 0</errorConditionFormula>
    <errorDisplayField>Desired_Monthly_Volume__c</errorDisplayField>
    <errorMessage>Monthly volume must be greater than 0</errorMessage>
</ValidationRule>